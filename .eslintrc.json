{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "prettier"],
  "overrides": [
    {
      "env": {
        "jasmine": true
      },
      "files": ["**/*.spec.js"]
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "spaced-comment": 0,
    "accessor-pairs": 0,
    "no-prototype-builtins": 0
  }
}
